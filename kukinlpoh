-- Load WindUI (Latest Stable)
local WindUI = loadstring(game:HttpGet(
    "https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"
))()

-- Create Window
local Window = WindUI:CreateWindow({
    Title = "Adopt Me | Jaraddz Hub",
    Icon = "laptop",
    Author = "made by Jaraddz",
    Folder = "JaraddzHubAdoptMe",
    Size = UDim2.fromOffset(520, 420),
    Transparent = true,
    Theme = "Dark",

    User = {
        Enabled = true,
        Callback = function() end,
        Anonymous = false
    },

    SideBarWidth = 150,
    ScrollBarEnabled = true
})

task.spawn(function()
    local CoreGui = game:GetService("CoreGui")

    local function nukeLeave(btn)
        if not btn then return end
        if btn.Name == "DontLeaveGameButton" or btn.Name == "LeaveGameButton" then
            pcall(function() btn:Destroy() end)
        end
    end
 ipairs(CoreGui:GetDescendants()) do
        nukeLeave(d)
    end
 CoreGui.DescendantAdded:Connect(function(inst)
        nukeLeave(inst)
    end)
end)

-------------------------------------------------------
--                TABS
-------------------------------------------------------

local OtherTab = Window:Tab({Title = "Other", Icon = "eye"})
local MM2Tab = Window:Tab({Title = "MM2", Icon = "skull"})
local ADMTab = Window:Tab({Title = "ADM", Icon = "heart"})


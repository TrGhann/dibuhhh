local function AddScripts(tab, section)
    for name, data in pairs(Scripts) do
        if data.section == section then
            tab:Button({
                Title = name,
                Desc = (data.desc or "No description") .. " ‚Ä¢ " .. (data.status or "Unknown"),
                Icon = "play",
                Callback = function()
                    scriptsLoadedCount = scriptsLoadedCount + 1
                    loadstring(data.script)()
                end
            })
        end
    end
end

-- Other Games Tab
OtherTab:Paragraph({
    Title = "Universal & Tools",
    Desc = "Universal hubs and utility scripts",
    Image = "package"
})

OtherTab:Section({ Title = "üîì No Key Required" })
for name, data in pairs(Scripts) do
    if data.section == "Other" and data.status ~= "üîë KEY SYSTEM" then
        OtherTab:Button({
            Title = name,
            Desc = (data.desc or "No description") .. " ‚Ä¢ " .. (data.status or "Unknown"),
            Icon = "play",
            Callback = function()
                scriptsLoadedCount = scriptsLoadedCount + 1
                loadstring(data.script)()
            end
        })
    end
end

OtherTab:Section({ Title = "üîë Key System Required" })
for name, data in pairs(Scripts) do
    if data.section == "Other" and data.status == "üîë KEY SYSTEM" then
        OtherTab:Button({
            Title = name,
            Desc = (data.desc or "No description") .. " ‚Ä¢ " .. (data.status or "Unknown"),
            Icon = "key",
            Callback = function()
                scriptsLoadedCount = scriptsLoadedCount + 1
                loadstring(data.script)()
            end
        })
    end
end


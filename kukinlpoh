-- Function to add scripts to a tab
local function AddScriptsToTab(tab, section)
    for name, data in pairs(Scripts) do
        if data.section == section then
            tab:Button({
                Title = name,
                Icon = "play",
                Callback = function()
                    loadstring(data.script)()
                    print("Executed script:", name)
                end
            })
        end
    end
end

-- Add scripts to each tab
AddScriptsToTab(OtherTab, "Other")
AddScriptsToTab(MM2Tab, "MM2")
AddScriptsToTab(ADMTab, "ADM")


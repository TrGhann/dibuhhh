-------------------------------------------------------
-- GAMING DASHBOARD TAB
-------------------------------------------------------
local startTime = tick()
local scriptsLoadedCount = 0

local function getPlayTime()
    local elapsed = tick() - startTime
    local hours = math.floor(elapsed / 3600)
    local minutes = math.floor((elapsed % 3600) / 60)
    local seconds = math.floor(elapsed % 60)
    return string.format("%02d:%02d:%02d", hours, minutes, seconds)
end

local function getServerRegion()
    local success, ping = pcall(function()
        return game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue()
    end)
    
    if not success then return "üü° Unknown" end
    
    if ping < 50 then return "üü¢ Excellent"
    elseif ping < 100 then return "üü° Good"
    elseif ping < 150 then return "üü† Fair"
    else return "üî¥ Poor" end
end

DashboardTab:Paragraph({
    Title = "üéÆ Gaming Dashboard",
    Desc = "Real-time stats and quick access controls",
    Image = "activity",
    Color = "Blue"
})

DashboardTab:Section({ Title = "üìä Session Stats" })

local serverInfoParagraph = DashboardTab:Paragraph({
    Title = "Server Information",
    Desc = "Server ID: " .. game.JobId:sub(1, 16) .. "...\n" ..
           "Players: " .. #Players:GetPlayers() .. "/" .. Players.MaxPlayers .. "\n" ..
           "Place ID: " .. game.PlaceId .. "\n" ..
           "Connection: " .. getServerRegion(),
    Color = "Green"
})

local sessionParagraph = DashboardTab:Paragraph({
    Title = "Your Session",
    Desc = "Play Time: " .. getPlayTime() .. "\n" ..
           "Scripts Loaded: " .. scriptsLoadedCount .. "\n" ..
           "Hub Version: v1.7.0\n" ..
           "Status: Active ‚úÖ",
    Color = "Blue"
})

-- Update stats every second
task.spawn(function()
    while task.wait(1) do
        pcall(function()
            sessionParagraph:Update({
                Desc = "Play Time: " .. getPlayTime() .. "\n" ..
                       "Scripts Loaded: " .. scriptsLoadedCount .. "\n" ..
                       "Hub Version: v1.7.0\n" ..
                       "Status: Active ‚úÖ"
            })
            
            serverInfoParagraph:Update({
                Desc = "Server ID: " .. game.JobId:sub(1, 16) .. "...\n" ..
                       "Players: " .. #Players:GetPlayers() .. "/" .. Players.MaxPlayers .. "\n" ..
                       "Place ID: " .. game.PlaceId .. "\n" ..
                       "Connection: " .. getServerRegion()
            })
        end)
    end
end)

